<h1>編集する</h1>
<%= form_for(@article) do |f| %>
  <%= render 'shared/error_messages', object: f.object %>
  <div class="field">
  	<%= f.label :title %><br />
    <%= f.text_field :title, placeholder: "Title here" %>
  </div>
  <div class="field">
  <%= f.label :content %><br />
  <%= f.text_area :content, placeholder: "content here" %>
  </div>
  <%= f.submit "投稿", class: "btn btn-primary" %>
<% end %>